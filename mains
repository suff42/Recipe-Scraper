    "servings": "8",
    "time": {
      "prep": "",
      "cook": "1 hour 40 minutes",
      "active": "",
      "inactive": "",
      "ready": "",
      "total": "1 hour 40 minutes"
    },
    "difficulty": "Super easy",
    "tags": [
      "Jamie's Dinners",
      "Chicken",
      "Dinner Party",
      "Sunday lunch",
      "Spanish",
      "Mains"
